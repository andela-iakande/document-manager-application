{
  "name": "document-manager-application",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production webpack --config webpack.config.js && NODE_ENV=production babel-watch ./server",
    "start:dev": "NODE_ENV=development babel-watch ./server",
    "webpack:dev": "webpack -w",
    "mocha": "NODE_ENV=test mocha --compilers js:babel-core/register server/tests/**/*.js --timeout 15000",
    "test": "NODE_ENV=test npm run test:scripts",
    "test-fe": "jest --watch client/tests/",
    "test:scripts": "npm run migrate:test:db && npm run coverage ",
    "migrate:db": "NODE_ENV=development sequelize db:migrate",
    "db:test:seed": "NODE_ENV=test sequelize db:seed:all",
    "db:test:development": "NODE_ENV=development sequelize db:seed:all",
    "db:test:drop": "NODE_ENV=test sequelize db:migrate:undo:all",
    "migrate:test:db": "NODE_ENV=test sequelize db:migrate",
    "frontend-test": "NODE_ENV=test mocha --compilers js:babel-core/register --require client/tests/components/setUp.js client/tests/**/*spec.js* ",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover  ./node_modules/.bin/_mocha  server/tests/**/**.spec.js -- --require babel-register",
    "document": "rimraf docs/ && jsdoc ./server/config/app.js -r -c ./conf.json --readme ./documentation/README.md --destination docs"
  },
  "keywords": [
    "git",
    "commit",
    "document",
    "npm"
  ],
  "author": "Akande Imisioluwa",
  "license": "ISC",
  "jest": {
    "setupFiles": [
      "./client/tests/components/setupFile.js"
    ]
  },
